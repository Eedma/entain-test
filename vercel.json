{
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/static-build"
        },
        {
            "src": "server/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "client/public/(.*)",
            "dest": "index.html"
        },
        {
            "src": "server/api/(.*)",
            "dest": "index.js"
        }
    ]
}
